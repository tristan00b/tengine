{
  "name": "TEngine Documentation",

  "entryPoints": [ "src/engine" ],
  "entryPointStrategy": "expand",
  "emit": "both",

  "out": "./build/public/docs",
  "cleanOutputDir": true,
  "githubPages": false,

  "excludeInternal": false,
  "excludeExternals": true,
  "externalPattern": [
    "**/build/**",
    "**/node_modules/**"
  ],

  "tsconfig": "./src/tsconfig.json",

  "theme": "default",

  "plugin": ["typedoc-plugin-missing-exports"]
}
