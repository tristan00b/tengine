{
  "name": "tengine",
  "version": "0.1.0",
  "description": "A 3D graphics engine for the web",
  "scripts": {
    "start": "gulp",
    "build": "gulp build --host",
    "docs": "typedoc",
    "test": "gulp test",
    "test:live": "gulp test --live",
    "clean": "rm -r ./build/public*",
    "clean:all": "rm -r ./build/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tristan00b/tengine.git"
  },
  "type": "module",
  "private": true,
  "keywords": [
    "3D Graphics",
    "Game Development",
    "Game Engine",
    "HTML5",
    "JavaScript",
    "WebGL",
    "WebGL2",
    "TypeScript"
  ],
  "author": {
    "name": "Tristan Bayfield",
    "url": "https://github.com/tristan00b"
  },
  "license": "AGPL-3.0",
  "typeCoverage": {
    "project": "./src/tsconfig.json",
    "atLeast": 96,
    "cache": true,
    "debug": false,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreTypeAssertion": true,
    "ignoreNonNullAssertion": true,
    "noDetailWhenFailed": false,
    "showRelativePath": true,
    "strict": true,
    "ignoreFiles": [
      "**/node_modules/**/*",
      "**/test/**/*"
    ]
  },
  "devDependencies": {
    "@types/gulp": "^4.0.9",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "browser-sync": "^2.27.7",
    "del": "^6.0.0",
    "esbuild": "^0.14.2",
    "eslint": "^8.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-ejs": "^5.1.0",
    "gulp-esbuild": "^0.10.0",
    "gulp-git": "^2.10.1",
    "gulp-jest": "^4.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-typedoc": "^3.0.1",
    "gulplog": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.4.3",
    "sass": "^1.44.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-plugin-type-coverage": "^2.18.3",
    "type-coverage": "^2.19.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typey-doo": "^0.3.0",
    "yargs": "^17.3.0"
  }
}
